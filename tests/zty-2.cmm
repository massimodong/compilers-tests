int swap(int a[100], int b[100], int len) {
  int pos = 0;
  while (pos < len) {
    int tmp = a[pos];
    a[pos] = b[pos];
    b[pos] = tmp;
    pos = pos + 1;
  }
  return len;
}

int main() {
  int a[100], b[100];
  int pos;

  pos = 0;
  while (pos < 100) {
    a[pos] = pos;
    pos = pos + 1;
  }

  pos = 0;
  while (pos < 100) {
    b[pos] = 100 - pos;
    pos = pos + 1;
  }

  pos = read();
  if (pos < 0 || pos > 99) {
    write(-1);
    return 0;
  }

  write(a[pos]);
  write(b[pos]);
  
  swap(a, b, 100);
  write(a[pos]);
  write(b[pos]);

  swap(a, b, pos);
  write(a[pos]);
  write(b[pos]);

  swap(a, b, pos + 1);
  write(a[pos]);
  write(b[pos]);

  return 0;
}
