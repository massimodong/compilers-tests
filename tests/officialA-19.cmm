struct food{
  int name;
  float price;
}ham, burger;

int tem;
float tem1;

int swap(struct food a, struct food b, int type){
  if(type == 1) {
    tem = a.name;
    a.name = b.name;
    b.name = tem;
  } else {
    tem1 = a.price;
    a.price = b.price;
    b.price = tem1;
  }
  return 1;
}

int main() {
  swap(ham,burger);
  return 0;
}
