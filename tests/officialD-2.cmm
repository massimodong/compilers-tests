int sequences[100][100];

int addEach(int p) {
  int i,j;
  i = 0;
  j = 0;
  while(i < 100){
    while(j < 100){
      sequences[i][j] = sequences[i][j] + p;
      j = j + 1;
    }
    i = i + 1;
  }
  return p;
}
int mulEach(int p) {
  int i, j;
  i = 0;
  j = 0;
  while(i < 100){
    while(j < 100){
      sequences[i][j] = sequences[i][j] * p;
      j = j + 1;
    }
    i = i + 1;
  }
  return p;

}

int main(){
  int i, j;
  i = 0;
  j = 0;
  while(i < 100){
    while(j < 100){
      sequences[i][j] = i * j;
      j = j + 1;
    }
    i = i + 1;
  }
  addEach(i + 3);
  mulEach(j - 4);
  return i + j;
}
