struct Fries{
  int number;
  float flavor;
};

int tasty(){
  struct Fries McDonald;
  McDonald.number = 30;
  McDonald.flavor = -1.2;
  while(McDonald.number > 13) {
    McDonald.flavor = McDonald.flavor + 1.2;
    McDonald.number = McDonald.number - 3;
  }

  if(McDonald.number <= 24) {
    return McDonald.number;
  } else{
    return McDonald.flavor;
  }
}
